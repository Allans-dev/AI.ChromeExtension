{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "Posts URL to database.",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "Favicon@2x.png",
    "default_title": "Add bookmark"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
